.theme-default-content  code {
    color: #d63200;
    padding: 3px 5px;
    margin: 0 2px;
    border-radius: 2px;
}
.theme-default-content:not(.custom) > h2{
    margin-bottom: 15px;
}
.theme-default-content:not(.custom) > h3{
    font-size: 14px;
    margin-bottom: 10px;
}
.theme-default-content>ul{
    padding-left: 0;
}

.theme-default-content>ul>li>p:first-child{
    font-weight: bold;
}
.theme-default-content>ul>li>p:first-child::before{
    content: "ü¶ç"
}

.theme-default-content>ul,.theme-default-content>div{
    overflow: auto;
}
.theme-default-content pre, .theme-default-content pre[class*="language-"]{
    padding: 0 1.5rem;
    }
.theme-default-content  div[class~="language-js"]{
    margin-bottom: 15px;}
.theme-default-content  div[class~="language-js"]:before {
    content:  "ÁÇπÂáªÂ§çÂà∂"
}
